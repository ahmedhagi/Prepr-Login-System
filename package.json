{
    "scripts": {
        "server": "npm start --prefix prepr-login-system-backend",
        "client": "npm start --prefix prepr-login-system-frontend",
        "client:build": "npm run build --prefix prepr-login-system-frontend",
        "prod:build": "concurrently \"npm run server\" \"npm run client:build\"",
        "start": "concurrently \"npm run server\" \"npm run client\""
    },
    "dependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "concurrently": "^9.1.0",
        "react-scripts": "^5.0.1"
    }
}
